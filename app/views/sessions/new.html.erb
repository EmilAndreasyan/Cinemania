<h1>Login</h1>

<p><i><%= @error %></i></p>

<%= form_tag ("/login") do %>
<%= label_tag :email %>
<%= email_field_tag :email %><br>
<%= label_tag :password %>
<%= password_field_tag :password %><br>
<%= submit_tag "Log In"%>
<% end %>

<p><i><%= link_to "Login in via GitHub", "/auth/github" %></i></p>

